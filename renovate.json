{
  "extends": ["config:base", "group:allNonMajor"],
  "baseBranches": [
    "dev"
  ],
  "reviewers": [
    "xknat"
  ],
  "ignoreDeps": [
    "com.viaversion:viaversion",
    "org.spigotmc:spigot-api"
  ],
  "labels": [
    "renovate"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ]
}
